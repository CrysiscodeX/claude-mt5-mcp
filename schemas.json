      "type": "object",
      "properties": {
        "symbol": {
          "type": "string"
        },
        "timeframe": {
          "type": "string"
        },
        "indicator": {
          "type": "string",
          "enum": [
            "SMA",
            "RSI"
          ]
        },
        "period": {
          "type": "integer"
        }
      },
      "required": [
        "symbol",
        "timeframe",
        "indicator",
        "period"
      ]
